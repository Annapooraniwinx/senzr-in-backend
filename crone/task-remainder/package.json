{
	"name": "task-remainder",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-operation"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.js",
			"api": "src/api.js"
		},
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "14.0.0",
		"vue": "^3.5.17"
	},
	"dependencies": {
		"dayjs": "^1.11.13",
		"jspdf-autotable": "^5.0.2",
		"nodemailer": "^7.0.5",
		"twilio": "^5.8.0"
	}
}
